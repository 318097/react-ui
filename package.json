{
  "name": "@codedrops/react-ui",
  "version": "3.0.17",
  "private": false,
  "description": "React UI library",
  "main": "dist/index.js",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.1.0",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.4",
    "node-sass": "^6.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "styled-components": "5.3.0",
    "uuid": "^8.3.2",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "dev": "webpack server --config webpack.app.js",
    "build:app": "rimraf './build' && webpack --config webpack.app.js",
    "build:lib": "webpack --config webpack.lib.js",
    "build:cdn": "rimraf './dist' && webpack --config webpack.cdn.js",
    "predeploy": "npm run build:lib",
    "patch": "npm version patch",
    "deploy": "npm run patch && npm run publish:npm",
    "publish:npm": "npm publish --access=public",
    "lint": "eslint --ext js,jsx src/UIComponents/",
    "fix": "eslint --fix src/UIComponents/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/318097/react-ui.git"
  },
  "author": "Mehul Lakhanpal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/318097/react-ui/issues"
  },
  "homepage": "https://github.com/318097/react-ui#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
